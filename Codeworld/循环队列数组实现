//设计循环队列
class MyCircularQueue {
     private int[] queue;
  private int headIndex;
  private int count=0;
  private int capacity;

    public MyCircularQueue(int k) {
        count=0;
        capacity=k;
        headIndex=0;
        queue=new int[k];
    }
    
    public boolean enQueue(int value) {
        if(capacity==count)
        return false;
        else{
            int tail=(count+headIndex)%capacity;
            queue[tail]=value;
            count++;
        }
        return true;
    }
    
    public boolean deQueue() {
        if(count==0){
            return false;
        }else{
           headIndex=(headIndex+1)%capacity;
           count--;
        }
        return true;

    }
    
    public int Front() {
        if(count==0){
            return -1;
        }else{
            return queue[headIndex];
        }

    }
    
    public int Rear() {
        if(count==0){
            return -1;
        }else{
            return queue[(headIndex+count-1)%capacity];
            
    }
    }
    
      public boolean isEmpty() {
        return (this.count==0);
    }  
    public boolean isFull() {
        return (this.count==capacity);
    }
}
